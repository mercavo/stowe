<tr id="<%%%= dom_id <%%= singular_table_name %> %>">
  <%%- attributes.each do |attribute| -%>
    <td>
      <%%%= link_to <%%= singular_table_name %>.<%%= attribute.name %>,  <%%= singular_table_name %>, class: 'btn' %>
    </td>
  <%%- end -%>
  <td width="180px">
    <%%%= render 'actions', <%%= singular_table_name %>: <%%= singular_table_name %> %>
  </td>
</tr>